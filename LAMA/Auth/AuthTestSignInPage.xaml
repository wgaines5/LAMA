<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LAMA.Auth.AuthTestSignInPage"
             Title="AuthTestPage">
    <VerticalStackLayout>

        <Entry Text="{Binding Email}" />
        <Entry IsPassword="True"  Text="{Binding Password}" />
        <Button Command="{Binding SignInCommand}" Text="Sign In" />
        <Button Command="{Binding NavigateSignUpCommand}" Text="Already have an account? Sign in" />
        
    </VerticalStackLayout>
</ContentPage>